{
  "name": "newbackend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "copy_config": "cp ../main_config.js ./config.js && echo 'module.exports = {adminComponents,Onces}' >> config.js",
    "create_database": "export $(cat ./.env-dev | xargs) && sh database/exec.sh database/create",
    "create_database_prod": "export $(cat ./.env-prod | xargs) && sh database/exec.sh database/create",
    "delete_database": "export $(cat ./.env-dev | xargs) && sh database/exec.sh database/delete",
    "reset_database": "npm run delete_database && npm run create_database",
    "start": "npm run copy_config && npm run docker",
    "launch": "export $(cat ./.env-dev | xargs) && nodemon main.js",
    "start42": "export $(cat ./.env-dev | xargs) && ./node_modules/nodemon/bin/nodemon.js main.js",
    "prod": "export $(cat ./.env-prod | xargs) && nohup node main.js &!",
    "killOld": "pkill -f 'node main.js'",
    "migrate_dtb": "export $(cat ./.env-dev | xargs) && knex migrate:latest",
    "rollback_dtb": "npm run create_database && export $(cat ./.env-dev | xargs) && knex migrate:rollback",
    "docker": "sudo docker-compose up",
    "kubernetes": "node main.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.392.0",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.15.0",
    "connect-mongo": "^2.0.3",
    "connect-redis": "^3.4.0",
    "country-list": "^1.1.1",
    "express": "^4.16.3",
    "express-async-errors": "^3.0.0",
    "express-fileupload": "^1.0.0-alpha.1",
    "express-http-proxy": "^1.5.1",
    "express-session": "^1.15.6",
    "file_utils_nithramir": "^1.0.2",
    "knex": "^0.15.2",
    "mongodb": "^3.1.13",
    "mysql": "^2.16.0",
    "nodemailer": "^6.4.16",
    "nodemon": "^1.18.9",
    "request": "^2.88.0",
    "simple_data_gen": "^1.2.1",
    "simple_paiement_express": "^0.1.2",
    "simple_user_express": "^1.1.3",
    "stripe": "^6.20.1",
    "superagent": "^4.1.0",
    "uuid": "^3.3.3"
  }
}
